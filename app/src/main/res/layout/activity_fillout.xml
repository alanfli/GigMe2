        android:layout_marginTop="423dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="107dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="184dp"
        android:layout_height="24dp"
        android:layout_marginLeft="35dp"
